{
  "name": "user-portal",
  "version": "1.0.0",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build-backup-server": "cd ../backup-server && yarn && tsc",
    "install-packages-in-user-portal": "yarn",
    "build-user-portal": "tsc -b",
    "run-user-portal": "node dist/index.js",
    "start": "npm-run-all -s build-backup-server install-packages-in-user-portal build-user-portal run-user-portal"
  },
  "dependencies": {
    "backup-server": "file:../backup-server"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "typescript": "^3.0.3"
  }
}
